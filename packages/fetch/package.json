{
  "name": "@logos-ui/fetch",
  "version": "2.0.1",
  "description": "A full-feature wrapper around the Fetch API",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "browserFuncName": "LogosUI.Fetch",
  "cdn": "dist/bundle.js",
  "scripts": {
    "build": "bash ../../scripts/build.zsh utils",
