{
  "name": "@logos-ui/kit",
  "version": "1.1.0",
  "description": "A collection of strongly-typed LogosUI libraries to kitstart your app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browserFuncName": "LogosUI.Kit",
  "cdn": "dist/bundle.js",
  "scripts": {
    "build": "sh ../../scripts/build.zsh utils",
    "types": "pnpm tsc --emitDeclarationOnly",
    "lint": "pnpm tsc --noEmit"
  },
  "files": [
    "dist/",
    "docs/",
    "readme.md",
    "LICENSE"
  ],
  "keywords": [
    "web apps",
    "boilerplate",
    "ui kit",
    "state manager",
    "observer",
    "event emitter",
    "localization",
    "localStorage",
    "sessionStorage",
    "app storage",
    "utilities",
    "fetch",
    "axios",
    "xhr requests",
    "node-fetch",
    "ajax"
  ],
  "homepage": "https://publish.obsidian.md/logos-ui/A+set+of+tools+for+sane+people",
  "bugs": {
    "url": "https://github.com/logos-ui/discuss/issues",
    "email": "danilo@alonso.network"
  },
  "author": "Danilo Alonso <danilo@alonso.network>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@logos-ui/dom": "workspace:^",
    "@logos-ui/fetch": "workspace:^",
    "@logos-ui/forms": "workspace:^",
    "@logos-ui/localize": "workspace:^",
    "@logos-ui/observer": "workspace:^",
    "@logos-ui/state-machine": "workspace:^",
    "@logos-ui/storage": "workspace:^",
    "@logos-ui/utils": "workspace:^"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.55",
    "typescript": "5"
  }
}
