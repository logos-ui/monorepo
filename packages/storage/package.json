{
  "name": "@logos-ui/storage",
  "version": "2.0.1",
  "description": "A feature-rich-but-simple, strongly-typed local storage wrapper",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "browserFuncName": "LogosUI.Storage",
  "cdn": "dist/bundle.js",
  "scripts": {
    "build": "bash ../../scripts/build.zsh utils",
    "types": "pnpm tsc --emitDeclarationOnly",
    "lint": "pnpm tsc --noEmit"
  },
